#include<bits/stdc++.h>
using namespace std;
int main()
{

    string a;
    cin>>a;

    for(int i=0;i<a.length();i++)
        {
          if(i%2==0)
          {
              if(a[i]=='R' || a[i]=='U' || a[i]=='D')
              {
                 continue;
              }
              else
              {
                cout<<"No"<<endl;
                return 0;
              }

          }
          else
          {
              if(a[i]=='L' || a[i]=='U' || a[i]=='D')
                {
                 continue;
                }
              else
              {
                cout<<"No"<<endl;
                return 0;
              }
          }
        }
          cout<<"Yes"<<endl;

    return 0;
}

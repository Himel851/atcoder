#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x;
    cin>>x;
    if(x<100)
    {
        if(x<10)
            cout<<x<<endl;
        else
            cout<<'9'<<endl;
    }
    else if(x>=100 && x<10000)
    {
        if(x<1000)
            cout<<9+(x-100)+1<<endl;
        else
            cout<<"909"<<endl;
    }
    else if(x>=10000 && x<1000000)
    {
        if(x<100000)
            cout<<909+(x-10000)+1<<endl;
        else
            cout<<"90909"<<endl;
    }
    return 0;
}
